#!/usr/bin/env bash
#
# Summary: Run source code

# Make call to src
src="$(bin/call src)"

# If call fails,
if [[ $? -ne 0 ]]; then
	# then exit
	ret=$?
	echo "Internal call to src failed." >&2
	exit $ret
fi

# If source code exists,
if [[ -f "$src" ]] ; then
	# then make subcall
	bin/subcall "$file" "$@"
else
	# else exit
	source bin/err/1
fi
